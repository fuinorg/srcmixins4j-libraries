<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Standard" location="www.fuin.org/files/checkstyle-2013-10-12.xml" type="project" description="">
    <property name="checkstyle.header.file" value="http://www.fuin.org/files/required-header-2013-10-12.txt"/>
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="fuin.org 2013-10-12" local="false">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="src/main/resources"/>
    <filter-data value="src/test/resources"/>
  </filter>
</fileset-config>
